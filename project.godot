[autoload]

VariableDebugger="*res://addons/variable_debugger/scenes/debug_overlay.tscn"

[editor_plugins]

enabled=PackedStringArray("res://addons/variable_debugger/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
